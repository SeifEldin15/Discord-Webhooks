{
  "manifest_version":3,
  "name":"EncoreExtension",
  "version":"1.8",
  "description":"Extension for links generated by Encore",
  "icons":{
    "400":"/images/encore.png"
  },
  "background":{
    "service_worker":"background.js"
  },
  "action":{
    
  },
  "permissions":[
    "cookies",
    "activeTab",
    "tabs",
	"webRequest",
    "webRequestAuthProvider",
	"proxy",
	"storage",
	"contextMenus"
  ],
  "host_permissions":[
    "*://*/*"
  ]
}